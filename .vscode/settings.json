{
    "files.exclude": {
        "**/.gitignore": true,
        "**/.gitpod.yml": true,
        "**/.npmrc": true,
        "**/.vscode": true,
        "**/*Dockerfile": true,
        "**/client.cnf": true,
        "**/config.inc.php": true,
        "**/databases_default": true,
        "**/mysql.cnf": true,
        "**/node_modules": true
    },
    "workbench.colorTheme": "One Dark Pro Flat",
    "workbench.preferredDarkColorTheme": "One Dark Pro Flat",
    "workbench.preferredLightColorTheme": "One Dark Pro Flat",
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "git.autofetch": true,
    "git.inputValidation": "off"
}